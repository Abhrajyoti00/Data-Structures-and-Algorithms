    "import math\n",
    "def LargestData(root):\n",
    "    if root == None:\n",
    "        return -math.inf\n",
    "    leftLar = LargestData(root.left)\n",
    "    rightlar = LargestData(root.right)\n",
    "    return max(root.data,leftLar,rightlar)"
   ]
