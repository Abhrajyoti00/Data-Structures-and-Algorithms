long long int t=1;
// cin>>t;
while(t--){
    long long int n;
    cin>>n;
    // long long int arr[] = {1,4,2,1,4,2,4,2,1,2};
    long long int arr[n];
    // long long int n = sizeof(arr)/sizeof(arr[0]);
    long long int ans = 0;
    for (long long int i = 0; i < n; i++)
    {
        cin>>arr[i];
    }
    ans = mergeSort(arr,0,n-1);
    cout<<"Sorted Array :- \n";
    for (long long int i = 0; i < n; i++)
    {
        cout<<arr[i]<<" ";
    }
   
    cout<<"\n Count of Inversions = "<<ans;



    
    
}
return 0;
}
