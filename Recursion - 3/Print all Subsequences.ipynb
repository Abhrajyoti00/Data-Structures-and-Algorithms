    "def printSubs(inp, out):\n",
    "    if len(inp) == 0:\n",
    "        print(out)\n",
    "        return\n",
    "    else:\n",
    "        smaller_inp = inp[1:]\n",
    "        printSubs(smaller_inp, out)\n",
    "        printSubs(smaller_inp, out + inp[0])\n",
    "\n",
    "\n",
    "str = input()\n",
    "printSubs(str, '')"
   ]
