bool isPrime(long long int n) 
{ 
    // Corner cases 
    if (n <= 1)  return false; 
    if (n <= 3)  return true; 
  
    // This is checked so that we can skip  
    // middle five numbers in below loop 
    if (n%2 == 0 || n%3 == 0) return false; 
  
    for (long long int i=5; i*i<=n; i=i+6) 
        if (n%i == 0 || n%(i+2) == 0) 
           return false; 
  
    return true; 
} 

long long merge(long long arr[], long long l, long long mid, long long r){
    // Create 2 temp arrays

    long long n1 = mid-l+1;
    long long n2 = r-mid;
    
    long long a[n1];
    long long b[n2]; 

    // We cannot merge long longo the same array. So we created two temp arrays

    for(long long i = 0; i<n1; i++)
        a[i] = arr[l+i];
    for(long long i = 0; i<n2; i++)
        b[i] = arr[mid+1+i];
    long long countofInv = 0;
    long long i = 0;
    long long j = 0;
    long long k = l;
    while(i<n1 && j< n2){
        if(a[i]<b[j]){
            arr[k] = a[i];
            k++;
            i++;
            // countofInv+=n2-j;
        }
        else{
            arr[k] = b[j];
            k++;
            j++;
            countofInv+=n1-i;

        }
    }

    while(i<n1){
        arr[k] = a[i];
        k++;
        i++;
    }
    while(j<n2){
        arr[k] = b[j];
        k++;
        j++;
    }
    return countofInv;
}

long long mergeSort(long long arr[], long long l, long long r){
    long long temp = 0;
    if(l<r){
        long long mid = l + (r-l)/2;
        temp+= mergeSort(arr,l,mid);
        temp+= mergeSort(arr,mid+1,r);

        temp+= merge(arr, l, mid, r);
    }
    return temp;
}

int32_t main(){
fast


// ||| Author: Abhrajyoti Kundu |||
